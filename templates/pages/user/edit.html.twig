{% extends 'base.html.twig' %}

{% block title %}Modification profile{% endblock %}

{% block body %}
<div style="padding-top:90px;"></div>
<div class="container">
    <h1 class="mt-4">Modification de votre profile utilisateur</h1>
    <div class="mt-4">
        {% for message in app.flashes('Danger')%}
            <div class="alert alert-warning mt-4">
                {{ message }}
            </div>
        {% endfor %}
        {{form(form)}}
    </div>
</div>
{% endblock %}
